{
  "env": {},
  "permissions": {
    "allow": [
      "Bash(pnpm type-check:*)",
      "Bash(pnpm type-check:h5:*)",
      "Bash(pnpm test:*)"
    ],
    "deny": [],
    "ask": []
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/stop-notify.sh"
          }
        ]
      }
    ],
    "PreToolUse": [],
    "PostToolUse": [],
    "Notification": [],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/task-analysis.sh"
          }
        ]
      }
    ]
  },
  "mcpServers": {},
  "contextProviders": [],
  "cleanupTempFilesOnExit": true,
  "enableTerminalShiftEnter": false
}
