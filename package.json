    "@dcloudio/uni-mp-xhs": "3.0.0-4080720251210001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-4080720251210001",
    "@vueuse/core": "^14.1.0",
    "alova": "^3.3.3",
    "bignumber.js": "^9.3.1",
    "crypto-js": "^4.2.0",
    "dayjs": "1.11.10",
    "js-base64": "^3.7.8",
    "js-md5": "^0.8.3",
    "lodash-es": "^4.17.22",
    "mockjs": "^1.1.0",
    "pinia": "2.0.36",
    "pinia-plugin-persistedstate": "3.2.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.1.9",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
