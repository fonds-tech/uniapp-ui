  },
  "overrides": {
    "unconfig": "7.3.2"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "unconfig": "7.3.2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write"
